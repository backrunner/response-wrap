{
  "name": "response-wrap",
  "version": "0.2.0",
  "description": "Simple response wrapper classes for Node.js servers",
  "main": "dist/response-wrap.umd.js",
  "module": "dist/response-wrap.esm.js",
  "typings": "dist/types/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "rollup -c rollup.config.ts -w",
    "build": "rimraf ./dist && tsc --module commonjs && rollup -c rollup.config.ts && rimraf ./dist/lib"
  },
  "author": "BackRunner",
  "license": "MIT",
  "homepage": "https://github.com/backrunner/response-wrap",
  "repository": {
    "type": "git",
    "url": "https://github.com/backrunner/response-wrap.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.54.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.10.0",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  }
}
